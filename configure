       lt_cv_prog_compiler_rtti_exceptions=yes
     fi
   fi
   $RM -r conftest*

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $lt_cv_prog_compiler_rtti_exceptions" >&5
